{
  "name": "uportlandia",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://uportlandia.uport.me",
  "repository": "https://github.com/uport-project/cleverland",
  "devDependencies": {
    "cloudfront-invalidate-cache": "^1.0.1",
    "react-scripts": "^2.1.8",
    "s3-deploy": "^1.2.1"
  },
  "dependencies": {
    "dayjs": "^1.8.0",
    "localforage": "^1.7.3",
    "mobile-detect": "^1.4.3",
    "qr-image": "^3.2.0",
    "qs": "^6.7.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-tooltip": "^3.9.2",
    "redux": "4.0.0",
    "redux-saga": "0.16.0",
    "reselect": "3.0.1",
    "shortid": "2.2.12",
    "styled-components": "4.1.2",
    "uport-credentials": "^1.1.5",
    "uport-transports": "^0.2.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "create-did": "node node_scripts/create_did.js",
    "s3-upload": "node_modules/.bin/s3-deploy './build/**' './build/.well-known/**' --cwd './build/' --region us-west-2 --bucket",
    "deploy:stage": "npm run build; npm run create-did stage; npm run s3-upload cleverland-stage && yarn invalidate uportlandia.uport.space",
    "deploy:production": "npm run build; npm run create-did production; npm run s3-upload cleverland-prod && yarn invalidate uportlandia.uport.me",
    "invalidate": "node_modules/.bin/cloudfront-invalidate-cache --cname"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
